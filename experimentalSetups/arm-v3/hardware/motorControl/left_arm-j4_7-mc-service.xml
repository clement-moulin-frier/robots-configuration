<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE params PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

<params robot="iCubGenova04" build="1">

    <group name="SERVICE">
        
        <param name="type"> eomn_serv_MC_mc4plus </param> 
    
        <group name="PROPERTIES">
        
            <group name="ETHBOARD">
                <param name="type">             mc4plus             </param> 
            </group>          

            
            

            <group name="JOINTMAPPING">

                <group name="ACTUATOR">                 
                    <param name="type">             pwm                 pwm                 pwm                 pwm                     </param>
                    <param name="port">             CONN:P2             CONN:P3             CONN:P4             CONN:P5                 </param>
                </group>
                
                <group name="ENCODER1">  
                    <param name="type">             qenc                aea                 aea                 absanalog               </param>  
                    <param name="port">             CONN:P2             CONN:P10            CONN:P11            CONN:P5                 </param>
                    <param name="position">         atmotor             atjoint             atjoint             atjoint                 </param>
                    <param name="resolution">       -2048                -4096               4096                65535                  </param> 
                    <param name="numofnoisebits">      0                3                   3                  0                        </param> 
                </group>        
                
                <group name="ENCODER2">
                    <param name="type">             none                qenc                qenc                none                    </param>
                    <param name="port">             CONN:none           CONN:P3             CONN:P4             CONN:none               </param>
                    <param name="position">         none                atmotor             atmotor                none                    </param>
                    <param name="resolution">       1                   1600                1600                    1                       </param>
                    <param name="numofnoisebits">      0                0                   0                  0                        </param> 
                </group>  
            </group>    
            
        </group>
           
    </group>
    
  
</params>

